{
	"env": {
		"browser": true,
		"es2021": true,
    "node": true
	},
	"extends": [
    "eslint:recommended",
		"plugin:prettier/recommended"
	],
	"parserOptions": {
		"ecmaVersion": 12
	},
  "parser": "babel-eslint",
	"plugins": ["prettier"],
	"rules": {
    "prefer-const": 2,
	  "prettier/prettier": "error"
	}
}
